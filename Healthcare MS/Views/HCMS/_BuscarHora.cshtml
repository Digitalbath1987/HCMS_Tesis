@model Healthcare_MS.Models.HoraMedica

<div id="resultado">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 class="modal-title">Consultar hora médica</h3>
    </div>
    <div class="modal-body">
        <div id="div-login-msg" class="row">
            <div id="icon-login-msg" class="glyphicon glyphicon-chevron-right"></div>
            <span id="text-login-msg">Resultado de búsqueda por código</span>
        </div>
        <div class="form-horizontal t-spacing">
            <div class="row">
                @Html.LabelFor(m => Model.Paciente.Persona.NombreCompleto, "Nombre completo", new { @class = "control-label col-sm-5" })
                <div class="col-sm-7">
                    @Html.TextBoxFor(m => m.Paciente.Persona.NombreCompleto, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>

            <div class="row">
                @Html.LabelFor(m => Model.Paciente.Persona.RutCompleto, "RUT", new { @class = "control-label col-sm-5" })
                <div class="col-sm-7">
                    @Html.TextBoxFor(m => m.Paciente.Persona.RutCompleto, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>

            <div class="row">
                @Html.LabelFor(m => Model.Paciente.Sector.Establecimiento.Nombre, "Centro médico", new { @class = "control-label col-sm-5" })
                <div class="col-sm-7">
                    @Html.TextBoxFor(m => m.Paciente.Sector.Establecimiento.Nombre, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>

            <div class="row">
                @Html.LabelFor(m => Model.Funcionario_Especialidad.Especialidad.Nombre, "Especialidad", new { @class = "control-label col-sm-5" })
                <div class="col-sm-7">
                    @Html.TextBoxFor(m => m.Funcionario_Especialidad.Especialidad.Nombre, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>

            <div class="row">
                @Html.LabelFor(m => Model.Funcionario_Especialidad.Funcionario.Persona.NombreCompleto, "Médico", new { @class = "control-label col-sm-5" })
                <div class="col-sm-7">
                    @Html.TextBoxFor(m => m.Funcionario_Especialidad.Funcionario.Persona.NombreCompleto, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>

            <div class="row">
                @Html.LabelFor(m => Model.FechaHoraCargada, "Fecha y hora solicitada", new { @class = "control-label col-sm-5" })
                <div class="col-sm-7">
                    @Html.TextBoxFor(m => m.fechaParseada, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>

            <div class="row">
                @Html.LabelFor(m => Model.EstadoHoraMedica.Nombre, "Estado", new { @class = "control-label col-sm-5" })
                <div class="col-sm-7">
                    @Html.TextBoxFor(m => m.EstadoHoraMedica.Nombre, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>
        </div>
    </div>

    <div class="modal-footer">
        <button type="button" class="btn btn-primary" id="back" data-dismiss="modal">Volver</button>
    </div>
</div>